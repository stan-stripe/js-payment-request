{
  "manifest_version": 2,

  "name": "JS PaymentRequest",
  "description": "This extension injects a JS implementation of PaymentRequest.",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["runner.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources":["index.js"]
}

